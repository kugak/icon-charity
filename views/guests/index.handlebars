    {{> _nav}}
    <div class="container">
        {{> _msg}}
        {{> _errors}}
        <h1>Guests</h1>
<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">First</th>
      <th scope="col">Last</th>
      <th scope="col">Company</th>
      <th scope="col">Email</th>
      <th scope="col">Receive Email</th>
    </tr>
  </thead>
  <tbody>
    {{#each guests}}
    <tr>
      <th scope="row">{{date}}</th>
      <td>{{first_name}}</td>
      <td>{{last_name}}</td>
      <td>{{company_name}}</td>
      <td><a href="mailto:{{email}}">{{email}}</a></td>
      <td>{{receive_email}}</td>
    </tr>
    {{else}}
<p>No Guests Yet!</p>
{{/each}}
  </tbody>
</table>
    </div>




